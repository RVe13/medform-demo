{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "LearnByDeclaring",
    "icon": "./assets/pill.png",
    "slug": "user",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9d071354-1563-43e1-9269-f86df5381d73"
      }
    },
    "android": {
      "package": "com.mediform.Client",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    }
  }
}
