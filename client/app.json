{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "MediForm Admin",
    "slug": "Admin",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "58bb20ad-5f3c-4516-bc3f-9cbca553c266"
      }
    },
    "android": {
      "package": "com.mediform.Admin"
    }
  }
}
